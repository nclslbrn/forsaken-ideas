        "postcss-preset-env": "^6.7.0",
        "pug-loader": "^2.4.0",
        "sass": "^1.29.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.3",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/preset-env": "^7.12.1",
        "@typescript-eslint/eslint-plugin": "^4.7.0",
        "@typescript-eslint/parser": "^4.7.0",
        "autoprefixer": "^10.0.1",
        "babel": "^6.23.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
