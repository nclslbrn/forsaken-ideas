{
    "name": "make",
    "version": "1.0.0",
    "description": "A tool to write p5js sketch",
    "main": "index.js",
    "scripts": {
        "watch:sketch": "webpack serve --config tasks/webpack.config.js --entry",
        "build:sketch": "webpack --mode production --config tasks/webpack.config.js --entry",
        "build:clean": "node tasks/node-clean.js",
        "build:sketches": "webpack --mode production --config tasks/webpack.export-all.js",
        "watch:gallery": "webpack serve --config tasks/webpack.gallery.js",
        "build:gallery": "webpack --mode production --config tasks/webpack.gallery.js --entry ./src/js/gallery.js",
        "build": "npm run build:clean && npm run build:sketches && npm run build:gallery",
        "primer": "node tasks/primer",
        "topics": "node tasks/getTopics",
        "deploy": "node tasks/node-deploy"
    },
    "keywords": [
        "p5js",
        "pug",
        "sass",
        "threejs",
        "webpack",
        "boilerplate",
        "gallery"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/nclslbrn/make.git"
    },
    "author": "Nicolas Lebrun <nl@artemg.com>",
    "license": "MIT",
    "dependencies": {
