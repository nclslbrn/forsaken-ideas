{
    "name": "make",
    "version": "1.0.0",
    "description": "A tool to write p5js sketch",
    "main": "index.js",
    "scripts": {
        "watch-sketch": "webpack serve --config tasks/webpack.config.js --entry",
        "export-sketch": "webpack --mode production --config tasks/webpack.config.js --entry",
        "clean-exports": "node tasks/node-clean.js",
        "export-all-sketches": "webpack --mode production --config tasks/webpack.export-all.js",
        "watch-gallery": "webpack serve --config tasks/webpack.gallery.js",
        "export-gallery": "webpack --mode production --config tasks/webpack.gallery.js --entry ./src/js/gallery.js",
        "export-all": "npm run clean-exports && npm run export-all-sketches && npm run export-gallery",
        "primer": "node tasks/primer",
        "deploy": "node tasks/node-deploy",
        "build": "npm run clean-exports && npm run export-all-sketches && npm run export-gallery"
    },
    "keywords": [
        "p5js",
        "pug",
        "sass",
        "threejs",
        "webpack",
        "boilerplate",
        "gallery"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/nclslbrn/make.git"
    },
    "author": "Nicolas Lebrun <nl@artemg.com>",
    "license": "MIT",
