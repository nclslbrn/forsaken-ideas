        "sass": "^1.66.1",
        "simplex-noise": "^4.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.22.11",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/preset-env": "^7.22.14",
        "autoprefixer": "^10.4.15",
        "babel": "^6.23.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^9.1.3",
        "babel-preset-env": "^1.7.0",
        "bezier-js": "^6.1.4",
