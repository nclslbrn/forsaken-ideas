{
    "name": "make",
    "version": "1.0.0",
    "description": "A workflow to build multiple js sketches/projects with",
    "main": "index.js",
    "scripts": {
        "watch": "webpack-dev-server",
        "export": "webpack -p --display-error-details",
        "export-all": "rimraf public/sketch/* && webpack --config webpack.export-all.js --display-error-details && webpack -p --config webpack.list-projects.js --display-error-details",
        "watch-index": "webpack-dev-server --config webpack.list-projects.js",
        "index": "webpack -p --config webpack.list-projects.js"
    },
    "keywords": [
        "p5js",
        "pug",
        "sass",
        "p5js",
        "threejs",
        "webpack",
        "boilerplate"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/nclslbrn/make.git"
    },
    "author": "Nicolas Lebrun <nclslbrn@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "@types/p5": "^0.9.1",
        "body-parser": "^1.19.0",
        "chromotome": "^1.18.1",
        "mini-css-extract-plugin": "^0.9.0",
        "p5": "^1.1.9",
        "p5.js-svg": "^0.5.2",
        "postcss-loader": "^3.0.0",
        "pug-loader": "^2.4.0",
        "sass": "^1.26.10"
    },
    "devDependencies": {
        "@babel/core": "^7.10.5",
        "@babel/preset-env": "^7.10.4",
        "@typescript-eslint/eslint-plugin": "^3.7.0",
        "@typescript-eslint/parser": "^3.7.0",
        "autoprefixer": "^9.8.5",
        "babel": "^6.23.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "babel-preset-env": "^1.7.0",
        "copy-webpack-plugin": "^6.0.3",
        "css-loader": "^3.6.0",
        "cssnano": "^4.1.10",
        "eslint": "^7.5.0",
        "eslint-config-p5js": "^1.0.1",
        "eslint-config-prettier": "^6.11.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.1",
        "exports-loader": "^1.1.0",
        "expose-loader": "^1.0.0",
        "fiber": "^1.0.4",
        "file-loader": "^6.0.0",
        "html-webpack-plugin": "^4.3.0",
        "imports-loader": "^1.1.0",
        "pug": "^3.0.0",
        "rimraf": "^3.0.2",
        "sass-loader": "^9.0.2",
        "svg.js": "^2.7.1",
        "three.js": "^0.77.1",
        "typescript": "^3.9.7",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0"
    }
}
