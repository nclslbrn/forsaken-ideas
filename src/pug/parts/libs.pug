mixin libs(property)
	if property.libs.includes('p5')
		script(src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.9.0/p5.min.js")

	if property.libs.includes("p5.dom")
		script(src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.9.0/addons/p5.dom.min.js")

	if property.libs.includes("p5.sound")
		script(src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.9.0/addons/p5.sound.min.js")

	if property.libs.includes("p5.collide2D")
		script(src="https://cdn.JsDelivr.net/gh/bmoren/p5.collide2D/p5.collide2d.min.js")

	if property.libs.includes("p5.js-svg")
		script(src="https://cdn.JsDelivr.net/gh/zenozeng/p5.js-svg/dist/p5.svg.js")

	if property.libs.includes('three')
		script(src="https://cdnjs.cloudflare.com/ajax/libs/three.js/109/three.js")

	if property.libs.includes('svg')
		script(src="https://cdnjs.cloudflare.com/ajax/libs/svg.js/3.0.13/svg.min.js")

	if property.libs.includes('fabric')
		script(src="https://cdnjs.cloudflare.com/ajax/libs/fabric.js/3.4.0/fabric.min.js")
	
	if property.libs.includes("p5.createloop")
		script(src="https://unpkg.com/p5.createloop@latest/dist/p5.createloop.js")
